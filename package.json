{
	"name": "nodefold",
	"version": "1.0.0",
	"description": "A Tool For Scaffolding Node Modules",
	"scripts": {
		"lint": "xo --fix",
		"test": "ava --verbose"
	},
	"directories": {
		"test": "test"
	},
	"author": {
		"name": "Samarth Verma",
		"email": "samarthverma0302@yahoo.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/clemtrek/nodefold.git"
	},
	"bugs": {
		"email": "samarthverma0302@yahoo.com",
		"url": "https://github.com/clemtrek/nodefold/issues"
	},
	"license": "MIT",
	"dependencies": {
		"caporal": "^0.7.0",
		"inquirer": "^3.3.0"
	},
	"devDependencies": {
		"@ava/babel-preset-stage-4": "^1.1.0",
		"ava": "^0.22.0",
		"babel-preset-env": "^1.6.0",
		"babel-register": "^6.26.0",
		"xo": "^0.18.2"
	},
	"ava": {
		"babel": "inherit",
		"require": [
			"babel-register"
		]
	},
	"xo": {
		"rules": {
			"require-jsdoc": [
				"error"
			],
			"valid-jsdoc": [
				"error"
			]
		}
	}
}
